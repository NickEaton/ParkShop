<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.BKConCompleteController">
   <children>
      <VBox layoutX="200.0" layoutY="200.0" prefHeight="600.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="500.0" spacing="20.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="You Created:">
                     <font>
                        <Font size="48.0" />
                     </font>
                  </Text>
                  <Text fx:id="bkName" strokeType="OUTSIDE" strokeWidth="0.0">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Text>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="400.0" prefWidth="500.0">
               <children>
                  <HBox fx:id="starBox" alignment="CENTER" prefHeight="50.0" prefWidth="500.0" translateY="-50.0" />
                  <ImageView fx:id="bkImage" fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
