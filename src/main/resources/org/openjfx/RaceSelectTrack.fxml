<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.RaceSelectTrackController">
   <children>
      <ScrollPane fx:id="scrollContent" prefHeight="800.0" prefWidth="400.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
        </content>
      </ScrollPane>
      <HBox alignment="CENTER" layoutY="800.0" prefHeight="100.0" prefWidth="400.0" spacing="50.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handoffRaceSelectRB" prefHeight="40.0" prefWidth="100.0" text="Confirm">
               <font>
                  <Font size="18.0" />
               </font></Button>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Cancel">
               <font>
                  <Font size="18.0" />
               </font></Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
